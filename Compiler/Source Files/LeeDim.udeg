programa TeTruena
	entero x;
	tipo entero Arre arreglo(1 hasta 10);
	Arre vector;
		principal
		inicio
			despliega("Indice al cual acceder: ");
			lee(x);
			despliega("Ingresa el valor de vector[ " + x + " ] :" + "\n");
			lee(vector[ x ]);
			despliega("Valor 1: " + vector[x] + "\n");
			vector[2] := 50;
			despliega("Valor 2: " + vector[2] + "\n");
			despliega("Valor 1: " + vector[x] + "\n");
		fin de principal;
	fin de TeTruena.